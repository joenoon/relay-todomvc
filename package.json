{
  "private": true,
  "scripts": {
    "lint": "eslint *.js src tools",
    "relay-compiler": "relay-compiler --src src --exclude 'data/**' --schema src/data/schema.graphql --language typescript --artifactDirectory ./src/__generated__",
    "start": "yarn run relay-compiler && babel-node --extensions '.ts,.tsx,.js,.jsx' src/server",
    "update-schema": "babel-node tools/updateSchema.js"
  },
  "lint-staged": {
    "*.js": [
      "yarn run update-schema",
      "git add src/data/schema.*"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
